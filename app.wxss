page {
  font-size: 30rpx;
  font-family: "Arial", "Microsoft YaHei", "黑体", "宋体", sans-serif;
}

.type-btn {
  display: flex;
  justify-content: center;
}
.type-btn .btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 680rpx;
  height: 90rpx;
  box-shadow: 0 0 16rpx 0 rgba(14, 47, 91, 0.5);
  border-radius: 45rpx;
  color: #ffffff;
  background-color: #f64a48;
}

.customer-service {
  position: fixed;
  right: 30rpx;
  bottom: 250rpx;
  width: 90rpx;
  height: 90rpx;
  padding: 0;
  border-radius: 50%;
  background: transparent;
}
.customer-service .icon {
  margin-left: -5rpx;
  margin-top: -5rpx;
  font-size: 100rpx;
  line-height: 100rpx;
  color: #f54847;
  background: #FFF;
}
