.null {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-top: 300rpx;
}
.null .null-img {
  width: 400rpx;
}
.null .null-desc {
  margin-top: 30rpx;
  font-size: 30rpx;
  color: #999999;
}

.pay {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 300rpx;
}
.pay .pay-success {
  width: 150rpx;
}
.pay .pay-desc {
  margin-top: 50rpx;
  font-size: 60rpx;
  color: #43bb85;
}

.look-order {
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100rpx;
  color: #ffffff;
  background-color: #f64a48;
}
